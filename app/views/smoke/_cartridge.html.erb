<div class="kalyan-item container cartridge col-sm-12 col-md-12 col-xs-12 col-lg-12 cart-<%= cartridge.id%>" >
  <div class="kalyan-image list">
    <%= image_tag cartridge.image, class: "cartridge-image list" %>
  </div>
  <div class="item-text">
    <h4 class="item-title"> <%= cartridge.name%> </h4>
    <p class="item-description"> <%= cartridge.cost%>  UAH/картридж </p>
    <a href=#>
      <div class="div-button">
        Купить
      </div>
    </a>
  </div>
</div>
